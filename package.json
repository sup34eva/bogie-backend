{
    "name": "bogie-backend",
    "version": "0.0.1",
    "description": "The service provider for the Bogie infrastructure",
    "main": "dist/index.js",
    "scripts": {
        "lint": "./node_modules/.bin/xo src/**/*.js",
        "prestart": "./node_modules/.bin/babel src --out-dir dist",
        "start": "node dist/index.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sup3bsc/bogie-backend.git"
    },
    "keywords": [
        "API",
        "Service"
    ],
    "author": "Sup3 Bsc",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/sup3bsc/bogie-backend/issues"
    },
    "homepage": "https://github.com/sup3bsc/bogie-backend#readme",
    "xo": {
        "esnext": true,
        "envs": [
            "node"
        ],
        "space": 4,
        "rules": {
            "prefer-arrow-callback": 1,
            "linebreak-style": [
                2,
                "windows"
            ]
        }
    },
    "devDependencies": {
        "pre-commit": "^1.1.2",
        "xo": "^0.12.1"
    },
    "dependencies": {
        "babel-cli": "^6.3.17",
        "babel-plugin-syntax-async-functions": "^6.3.13",
        "babel-plugin-transform-es2015-destructuring": "^6.4.0",
        "babel-plugin-transform-es2015-modules-commonjs": "^6.4.5",
        "babel-plugin-transform-es2015-parameters": "^6.4.5",
        "babel-plugin-transform-es2015-spread": "^6.4.0",
        "babel-plugin-transform-object-rest-spread": "^6.3.13",
        "babel-plugin-transform-regenerator": "^6.3.18",
        "babel-polyfill": "^6.3.14",
        "dataloader": "^1.0.0",
        "express-graphql": "^0.4.5",
        "graphql": "^0.4.14",
        "graphql-relay": "^0.3.6",
        "koa": "^2.0.0-alpha.3",
        "koa-bodyparser": "^3.0.0",
        "koa-morgan": "^1.0.1",
        "koa-router": "^7.0.1",
        "mongoose": "^4.3.2",
        "pg": "^4.4.3",
        "pg-hstore": "^2.3.2",
        "sequelize": "^3.14.2"
    },
    "pre-commit": [
        "lint"
    ]
}
